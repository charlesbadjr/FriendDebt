

<h1>Write Us: </h1>
<div class="ui form" >
   <%= form_for @feedback do |f| %> 
   <div class="feild"> 
     <%= f.label "Name"  %>  
     <br/>
     <%= f.text_field :name, :required => true, autofocus: true %>
   </div>
   <div class="feild"> 
     <%= f.label "Email"  %>
     <br/>
     <%= f.text_field :email %> 
   </div>
   <div class="feild">
      <%= f.label "Phone Number"  %>
      <br/>
      <%= f.text_field :phone_number %> 
   </div> 
   <div class="feild">
      <%= f.label "Comment"  %>
      <br/>
      <%= f.text_area :comments, :required => true %> 
   </div>
    <%= f.submit "Submit", class: 'ui button primary' %> 
</div>
<% end %>